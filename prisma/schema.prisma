generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int       @id @default(autoincrement())
  name     String
  number   String
  Contacts Contact[]
}

model Contact {
  id      Int    @id @default(autoincrement())
  id_user Int
  name    String
  number  String
  status  String
  user    User   @relation(fields: [id_user], references: [id])
}
